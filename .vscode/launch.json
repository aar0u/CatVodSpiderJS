{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "testRunner",
      "program": "${workspaceFolder}/tester/testRunner.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal"
    },
    {
      "name": "npm dev",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "cwd": "${workspaceFolder}/proxy",
      "console": "integratedTerminal"
    }
  ]
}
